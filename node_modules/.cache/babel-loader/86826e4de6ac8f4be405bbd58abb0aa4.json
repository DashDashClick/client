number\n        console.log(this.number,'menang');\n      }\n    },\n      countDownTimer() {\n        if(this.countdown > 0) {\n        console.log('jalan');\n          setTimeout(() => {\n              this.countdown -= 1\n              this.countDownTimer()\n          }, 1000)\n        }\n      }\n  },\n  created:function(){\n    \n  }\n}\n</script>\n<style scoped>\n\n</style>\n"],"sourceRoot":"src/views"}]}